{
  "compilerOptions": {
    "target": "ESNext",
    "isolatedModules": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "strict": true,
    "module": "ES2022",
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "types": ["vite/client"],
  }
}